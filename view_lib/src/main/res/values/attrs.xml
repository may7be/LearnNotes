    <declare-styleable name="CustomPressPlateSumView">
        <attr name="progressBackgroundHeight" />
        <attr name="bottomBackgroundHeight" />
        <attr name="progressBackgroundColor" />
        <attr name="bottomBackgroundColor" />
        <attr name="middleSpace" />
        <attr name="scaleTextSize" />
        <attr name="secondProgressBackgroundColor" />
        <attr name="scaleWidth" />
        <attr name="smallBlockColor" />
        <attr name="bigBlockColor" />
        <attr name="bottomScaleTextColor" />
        <attr name="topScaleTextColor" />
    </declare-styleable>

    <declare-styleable name="CustomTimeAxisView">
        <attr name="scaleLeftMargin" format="dimension" />
        <attr name="scaleRightMargin" format="dimension" />
    </declare-styleable>

    <!--低压空压机状态区间自定义view-->
    <declare-styleable name="CustomStatusSectionView">
        <attr name="backGroundHeight" format="dimension" />
    </declare-styleable>

    <!-- 启停仓储情况圆环 -->
    <declare-styleable name="StartAndStopStorageCircle">
        <attr name="outCirqueOutRadium" format="dimension" />
        <attr name="outCirqueWidth" format="dimension" />
        <attr name="inCirqueOutRadium" format="dimension" />
        <attr name="inCirqueWidth" format="dimension" />
        <attr name="outCirqueNormalColor" format="color" />
